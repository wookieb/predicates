{
    "name": "predicates",
    "version": "2.0.0-rc1",
    "description": "Set of various predicates for type checking, assertions, filtering etc",
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/wookieb/predicates"
    },
    "scripts": {
        "compile": "rm -rf compiled && ./node_modules/.bin/tsc -p ./.config/tsconfig.json",
        "prepublish": "npm run compile",
        "prepack": "cp -r ./compiled/* .",
        "postpack": "rm -f *.d.ts && rm -f *.js && rm -rf integer && rm -rf utils",
        "lint": "./node_modules/.bin/tslint --config ./.config/tslint.json src/*.ts",
        "test": "__TS_CONFIG__=./config/tsconfig-test.json ./node_modules/.bin/jest --config ./.config/jest.json",
        "test:ci": "./node_modules/.bin/jest --config ./.config/jest.json --testResultsProcessor ./node_modules/jest-junit"
    },
    "engines": {
        "node": ">=4.3"
    },
    "keywords": [
        "predicates",
        "predicate",
        "type",
        "check",
        "assert",
        "filter"
    ],
    "files": [
        "**/*.d.ts",
        "**/*.js"
    ],
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^3.5.2",
        "@types/jest": "^19.2.4",
        "@types/sinon": "^2.2.1",
        "chai": "^1.10.0",
        "jest": "^20.0.0",
        "jest-junit": "^1.5.1",
        "sinon": "^2.2.0",
        "ts-jest": "^20.0.0",
        "tslint": "^5.2.0",
        "typescript": "^2.3.2"
    },
    "author": "Łukasz Kużyński",
    "license": "MIT"
}
